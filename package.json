{
	"name": "@andersgee/pkg-demo",
	"author": "andersgee",
	"description": "Example npm app",
	"license": "MIT",
	"version": "0.0.6",
	"type": "module",
	"scripts": {
		"build": "rm -rf ./dist && tsc",
		"start": "node dist/index.js",
		"bs": "pnpm build && pnpm start",
		"dev": "tsc-watch --onSuccess \"pnpm start\"",
		"release": "pnpm run build && changeset publish",
		"lint": "eslint src --ext .ts"
	},
	"imports": {
		"#src/*": "./dist/*"
	},
	"devDependencies": {
		"@types/eslint": "^8.44.7",
		"@types/node": "^20.9.3",
		"@typescript-eslint/eslint-plugin": "^6.12.0",
		"@typescript-eslint/parser": "^6.12.0",
		"eslint": "^8.54.0",
		"prettier": "^3.1.0",
		"tsc-watch": "^6.2.0",
		"typescript": "5.2.2"
	}
}
